{
  "name": "foundation-spa-frontend",
  "version": "1.0.0",
  "description": "Frontend for Episerver Foundation CMS built using TypeScript & React",
  "private": true,
  "scripts": {
    "sync-models": "npx epi-sync-models",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "echo \"Clean: not implemented\" && exit 0",
    "build": "echo \"Development: Not synchronizing models prior to build!\" && webpack --env.NODE_ENV=development --mode development --config webpack.config.js && webpack --env.NODE_ENV=development --mode development --config server.webpack.config.js",
    "release": "npx epi-sync-models && webpack --env.NODE_ENV=production --mode production --config webpack.config.js && webpack --env.NODE_ENV=production --mode production --config server.webpack.config.js",
    "watch-browser": "npx epi-sync-models && webpack --watch --progress --env.NODE_ENV=development --mode development --config webpack.config.js",
    "watch-server": "npx epi-sync-models && webpack --watch --progress --env.NODE_ENV=development --mode development --config server.webpack.config.js",
    "compile": "npx epi-sync-models && tsc -p .\\tsconfig.json"
  },
  "author": "Remko Jantzen",
  "license": "Apache-2.0",
  "dependencies": {
    "@episerver/spa-core": "git+https://github.com/episerver/foundation-lib-spa-core.git",
    "@reduxjs/toolkit": "^1.3.6",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "core-js": "^3.6.5",
    "jquery": "^3.5.1",
    "lodash": "^4.17.21",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-redux": "^7.2.0",
    "reactstrap": "^8.4.1"
  },
  "devDependencies": {
    "@episerver/webpack": "git+https://github.com/episerver/foundation-lib-spa-webpack.git",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@types/bootstrap": "^4.3.3",
    "@types/core-js": "^2.5.3",
    "@types/jquery": "^3.3.38",
    "@types/lodash": "^4.14.155",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-redux": "^7.1.8",
    "@types/reactstrap": "^8.4.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^2.3.6",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
