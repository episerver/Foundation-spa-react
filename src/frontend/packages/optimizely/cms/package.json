{
  "name": "@optimizely/cms",
  "version": "0.1.2",
  "description": "React Library for Optimizely CMS",
  "main": "dist/index.js",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prebuild": "yarn shx rm -rf ./dist ./cjs",
    "build": "yarn tsc --build ./tsconfig.cjs.json ./tsconfig.json",
    "postbuild": "yarn renamer --find .js --replace .cjs --path-element ext ./cjs/**/*.js && yarn opti-cjsUpdate ./cjs/**/*.cjs",
    "compile": "yarn tsc --build ./tsconfig.json",
    "watch": "yarn tsc --build ./tsconfig.json --watch",
    "compile-cjs": "yarn tsc --build ./tsconfig.cjs.json",
    "watch-cjs": "yarn tsc --build ./tsconfig.cjs.json --watch"
  },
  "keywords": [
    "Optimizely",
    "CMS"
  ],
  "author": "Remko Jantzen",
  "license": "Apache-2.0",
  "peerDependencies": {
    "cross-fetch": "^3.0.6",
    "encoding": "^0.1.13",
    "react": "^18.0.0",
    "swr": "^1.2.2",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@optimizely/build-tools": "workspace:*",
    "@types/node": "^17",
    "@types/react": "^18",
    "cross-fetch": "^3.0.6",
    "encoding": "^0.1.13",
    "react": "^18.0.0",
    "renamer": "^4.0.0",
    "shx": "^0.3.4",
    "swr": "^1.2.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
