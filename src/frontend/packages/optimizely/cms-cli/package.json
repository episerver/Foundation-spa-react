    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/glob": "^7.2.0",
    "@types/yargs": "^17.0.8",
    "ansi-styles": "^6.1.0",
    "chalk": "^5",
    "cli-table3": "^0.6.1",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "glob": "^8.0.0",
    "node-fetch": "^3.2.0",
    "open": "^8.4.0",
    "openid-client": "^5.1.2",
    "rollup": "^2.67.0",
