    "yargs": "^17.3.1"
  },
  "peerDependencies": {
    "@optimizely/cms": "workspace:*",
    "ansi-styles": "*",
    "chalk": "^5",
    "cli-table3": "^0.6.1",
    "dotenv": "^16.0.2",
    "dotenv-expand": "^10.0.0",
    "glob": "^7.2.0",
