    "@types/glob": "^8.0.1",
    "@types/yargs": "^17.0.22",
    "ansi-styles": "^6.2.1",
    "chalk": "^5.2.0",
    "cli-table3": "^0.6.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "glob": "^8.1.0",
    "node-fetch": "^3.3.0",
    "open": "^8.4.1",
