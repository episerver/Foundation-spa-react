{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "EPiServer": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "urls": "http://*:8000/;https://*:8001/;",
  "AllowedHosts": "*",
  "EPiServer": {
    "CmsUI": {
      "Upload": {
        "FileSizeLimit": 104857600
      }
    },
    "Find": {
      "DefaultIndex": "changeme",
      "ServiceUrl": "https://example.com/changeme/",
      "TrackingSanitizerEnabled": true,
      "TrackingTimeout": 30000
    },
    "OdpVisitorGroupOptions": {
      "OdpCookieName": "vuid",
      "CacheTimeoutSeconds": 1,
      "BaseEndPoint": "https://api.example.com",
      "PrivateApiKey": "changeme"
    }
  },
  "Optimizely": {
    "Query": {
      "GatewayAddress": "https://example.com",
      "AppKey": "changeme",
      "Secret": "changeme",
      "SingleKey": "changeme",
      "AllowSendingLog": "true"
    }
  },
  "Foundation": {
    "Certificates": {
      "UseDevelopmentCertificate": true
    }
  },
  "NodeJsMiddleware": {
    "Disabled": "true",
    "Port": 3000,
    "FrontendPath": "..\\frontend",
    "AutoStart": "false"
  }
}
